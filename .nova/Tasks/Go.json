{
  "actions" : {
    "build" : {
      "enabled" : true,
      "script" : "goimports -w . && go build && go vet .\/... && go test .\/..."
    },
    "clean" : {
      "enabled" : true,
      "script" : "go clean"
    },
    "run" : {
      "enabled" : true,
      "script" : "go run ."
    }
  },
  "openLogOnRun" : "start"
}
